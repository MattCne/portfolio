{# src/Acme/BlogBundle/Resources/views/Blog/index.html.twig #}
{% extends 'PortfolioBundle::base.html.twig' %}


{% block body %}
    <div class="full full-home">
        {% if home is not null %}
            <header>
                <h1>{{ home.titre }}</h1>
                <div class="accroche content middle">
                    {{ home.accroche | raw }}
                </div>
            </header>
        {% endif %}
    </div>

{% endblock %}